#: src/pam_pwquality.c:25
#, c-format
msgid "New %s%spassword: "
msgstr "Yeni %s%sparolası: "

#: src/pam_pwquality.c:27
#, c-format
msgid "Retype new %s%spassword: "
msgstr "Yeni %s%sparolasını tekrar girin: "

#: src/pam_pwquality.c:28
msgid "Sorry, passwords do not match."
msgstr "Üzgünüm, parolalar birbirine uymuyor."

#: src/pam_pwquality.c:241
#, c-format
msgid "BAD PASSWORD: %s"
msgstr "KÖTÜ PAROLA: %s"

#: src/pwscore.c:22
#, c-format
msgid "Usage: %s [user]\n"
msgstr "Kullanımı: %s [user]\n"

#: src/pwscore.c:23
#, c-format
msgid ""
"       The command reads the password to be scored from the standard "
"input.\n"
msgstr ""
"       Komut, standart girişten okunacak parolayı okur.\n"

#: src/pwscore.c:51 src/pwscore.c:59 src/pwscore.c:65
#, c-format
msgid "Error: %s\n"
msgstr "Hata: %s\n"

#: src/pwscore.c:51
msgid "Could not obtain the password to be scored"
msgstr "Puanlandırılacak şifre alınamadı"

#: src/pwscore.c:73
#, c-format
msgid ""
"Password quality check failed:\n"
" %s\n"
msgstr ""
"Şifre kalite kontrolü başarısız oldu:\n"
" %s\n"

#: src/pwmake.c:22
#, c-format
msgid "Usage: %s <entropy-bits>\n"
msgstr "Kullanımı: %s <entropy-bits>\n"

#: src/error.c:31
msgid "Memory allocation error when setting"
msgstr "Ayarlandığında bellek ayırma hatası"

#: src/error.c:35
msgid "Memory allocation error"
msgstr "Bellek ayırma hatası"

#: src/error.c:37
msgid "The password is the same as the old one"
msgstr "Şifre eski olanla aynıdır"

#: src/error.c:39
msgid "The password is a palindrome"
msgstr "Şifre bir palindrom"

#: src/error.c:41
msgid "The password differs with case changes only"
msgstr "Şifre sadece vaka değişiklikleriyle değişir"

#: src/error.c:43
msgid "The password is too similar to the old one"
msgstr "Şifre eskisine çok benzer"

#: src/error.c:45
msgid "The password contains the user name in some form"
msgstr "Şifre, kullanıcı adını içeriyor"

#: src/error.c:47
msgid ""
"The password contains words from the real name of the user in some form"
msgstr "Parola, kullanıcının gerçek adından sözcükler içerir"

#: src/error.c:49
msgid "The password contains forbidden words in some form"
msgstr "Şifre, bazı formlarda yasaklanmış kelimeler içeriyor"

#: src/error.c:52
#, c-format
msgid "The password contains less than %ld digits"
msgstr "Şifre %ld'den az rakam içeriyor"

#: src/error.c:55
msgid "The password contains too few digits"
msgstr "Şifre çok az sayı içeriyor"

#: src/error.c:58
#, c-format
msgid "The password contains less than %ld uppercase letters"
msgstr "Şifre %ld'den büyük harfler içerir."

#: src/error.c:61
msgid "The password contains too few uppercase letters"
msgstr "Şifre çok az sayıda büyük harf içeriyor"

#: src/error.c:64
#, c-format
msgid "The password contains less than %ld lowercase letters"
msgstr "Şifre %ld'den küçük harfler içerir."

#: src/error.c:67
msgid "The password contains too few lowercase letters"
msgstr "Şifre çok az sayıda küçük harf içeriyor"

#: src/error.c:70
#, c-format
msgid "The password contains less than %ld non-alphanumeric characters"
msgstr "Parola %ld'den az alfasayısal olmayan karakterler içeriyor"

#: src/error.c:73
msgid "The password contains too few non-alphanumeric characters"
msgstr "Şifre çok az alfasayısal olmayan karakter içeriyor"

#: src/error.c:76
#, c-format
msgid "The password is shorter than %ld characters"
msgstr "Şifre %ld karakterden kısa"

#: src/error.c:79
msgid "The password is too short"
msgstr "Şifre çok kısa"

#: src/error.c:81
msgid "The password is just rotated old one"
msgstr "Şifre sadece eski bir tane döndürülüyor"

#: src/error.c:84
#, c-format
msgid "The password contains less than %ld character classes"
msgstr "Şifre sadece eski bir tane döndürülüyor"

#: src/error.c:87
msgid "The password does not contain enough character classes"
msgstr "Şifre yeterli karakter sınıfı içermiyor"

#: src/error.c:90
#, c-format
msgid "The password contains more than %ld same characters consecutively"
msgstr "Parola art arda %ld'den fazla karakter içeriyor"

#: src/error.c:93
msgid "The password contains too many same characters consecutively"
msgstr "Parola art arda çok fazla aynı karakter içeriyor"

#: src/error.c:96
#, c-format
msgid ""
"The password contains more than %ld characters of the same class "
"consecutively"
msgstr "Parola art arda aynı sınıfın %ld karakterlerini içerir."

#: src/error.c:99
msgid ""
"The password contains too many characters of the same class consecutively"
msgstr "Şifre, aynı sınıfın ardışık olarak çok fazla karakter içeriyor"

#: src/error.c:102
#, c-format
msgid "The password contains monotonic sequence longer than %ld characters"
msgstr "Parola %ld karakterden uzun monotonik diziyi içerir."

#: src/error.c:105
msgid "The password contains too long of a monotonic character sequence"
msgstr "Şifre çok uzun bir monoton karakter dizisi içeriyor"

#: src/error.c:107
msgid "No password supplied"
msgstr "Parola girilmedi"

#: src/error.c:109
msgid "Cannot obtain random numbers from the RNG device"
msgstr "RNG cihazından rasgele sayılar elde edilemiyor"

#: src/error.c:111
msgid "Password generation failed - required entropy too low for settings"
msgstr "Şifre oluşturma başarısız oldu - ayarlar için gerekli entropi gerekli değil"

#: src/error.c:114 src/error.c:117
msgid "The password fails the dictionary check"
msgstr "Parola sözlük denetimi başarısız olur"

#: src/error.c:120 src/error.c:124
msgid "Unknown setting"
msgstr "Bilinmeyen ayar"

#: src/error.c:127
msgid "Bad integer value of setting"
msgstr "Ayarın hatalı tam sayı değeri"

#: src/error.c:131
msgid "Bad integer value"
msgstr "Hatalı tam sayı değeri"

#: src/error.c:134
#, c-format
msgid "Setting %s is not of integer type"
msgstr "%s ayarı tamsayı tipinde değil"

#: src/error.c:138
msgid "Setting is not of integer type"
msgstr "Ayar tamsayı tipinde değil"

#: src/error.c:141
#, c-format
msgid "Setting %s is not of string type"
msgstr "%s ayarı dize türünde değil"

#: src/error.c:145
msgid "Setting is not of string type"
msgstr "Ayar dize türü değil"

#: src/error.c:147
msgid "Opening the configuration file failed"
msgstr "Yapılandırma dosyasını açma başarısız oldu"

#: src/error.c:149
msgid "The configuration file is malformed"
msgstr "Yapılandırma dosyası bozuk"

#: src/error.c:151
msgid "Fatal failure"
msgstr "Ölümcül hata"

#: src/error.c:153
msgid "Unknown error"
msgstr "Bilinmeyen hata"
