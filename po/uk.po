#~ msgstr "Новий пароль %s%s:"

#~ msgid "Retype new %s%spassword: "
#~ msgstr "Повторіть новий пароль %s%s: "

#~ msgid "Sorry, passwords do not match."
#~ msgstr "Вибачте, паролі не збігаються."
